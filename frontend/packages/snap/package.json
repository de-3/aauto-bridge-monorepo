{
  "name": "aauto-bridge-metamask-snap",
  "version": "0.1.0",
  "private": false,
  "description": "AccountAbstracted-Auto-Bridge MetaMask snap, enabling automated ETH bridge to L2 networks",
  "repository": {
    "type": "git",
    "url": "https://github.com/de3-masterhack/aauto-bridge-monorepo.git"
  },
  "license": "UNLICENSED",
  "main": "src/index.ts",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build": "mm-snap build",
    "build:clean": "pnpm clean && pnpm build",
    "clean": "rimraf dist",
    "lint": "pnpm lint:eslint && pnpm lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "pnpm lint:eslint --fix && pnpm lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "serve": "mm-snap serve",
    "start": "mm-snap watch",
    "test": "jest",
    "format": "eslint --fix './**/*.{ts,tsx}' && prettier --write './**/*.{ts,tsx,yaml,json}'"
  },
  "dependencies": {
    "@metamask/snaps-types": "^0.32.2",
    "@metamask/snaps-ui": "^0.32.2",
    "buffer": "^6.0.3",
    "ethers": "^5.7.2",
    "userop": "^0.3.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@lavamoat/allow-scripts": "^2.0.3",
    "@metamask/auto-changelog": "^2.6.0",
    "@metamask/eslint-config": "^10.0.0",
    "@metamask/eslint-config-jest": "^10.0.0",
    "@metamask/eslint-config-nodejs": "^10.0.0",
    "@metamask/eslint-config-typescript": "^10.0.0",
    "@metamask/snaps-cli": "^0.32.2",
    "@metamask/snaps-jest": "^0.35.2-flask.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.2.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "rimraf": "^3.0.2",
    "through2": "^4.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.7.4"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
